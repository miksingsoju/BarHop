        android:layout_width="259dp"
        android:layout_height="68dp"
        android:text="Edit Bar"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/createBarReturnButton"
        app:layout_constraintHorizontal_bias="0.008"
        app:layout_constraintStart_toStartOf="@+id/createBarReturnButton"
        app:layout_constraintTop_toBottomOf="@+id/createBarReturnButton"
        app:layout_constraintVertical_bias="0.028" />

    <Button
        android:id="@+id/userSettingsReturnButton"
        style="@style/Widget.Material3.Button.IconButton.Filled"
        android:layout_width="83dp"
        android:layout_height="48dp"
        android:text="Return"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />


