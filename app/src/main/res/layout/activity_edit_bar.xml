        app:layout_constraintEnd_toEndOf="@+id/createBarLabel"
        app:layout_constraintHorizontal_bias="0.051"
        app:layout_constraintStart_toStartOf="@+id/createBarLabel"
        app:layout_constraintTop_toBottomOf="@+id/createBarLabel" />

    <EditText
        android:id="@+id/createBarNameField"
        android:layout_width="356dp"
        android:layout_height="55dp"
        android:ems="10"
        android:inputType="text"
        android:hint="Bar Name"
        app:layout_constraintEnd_toEndOf="@+id/createBarNameLabel"
        app:layout_constraintHorizontal_bias="0.0"
