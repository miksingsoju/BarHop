<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.AdminPanel">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/adminsList"
        android:layout_width="333dp"
        android:layout_height="544dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="@+id/userWelcomeLabel"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="@+id/userWelcomeLabel"
        app:layout_constraintTop_toBottomOf="@+id/userWelcomeLabel" />

    <TextView
        android:id="@+id/userWelcomeLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="Admin Panel"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>